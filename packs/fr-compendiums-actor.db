e":0,"units":"","damage":""},"long":{"value":0,"units":"","damage":""},"extreme":{"value":0,"units":"","damage":""}},"usesPerRound":{"normal":"1","max":"","burst":null},"bullets":"","ammo":0,"malfunction":"","blastRadius":null,"properties":{"melee":false,"rngd":false,"thrown":false,"shotgun":false,"dbrl":false,"impl":false,"brst":false,"auto":false,"ahdb":false,"addb":false,"slnt":false,"spcl":true,"mont":false,"blst":false,"stun":false,"rare":false,"burn":false},"price":{}},"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}],"effects":[],"folder":null,"sort":0,"flags":{"cf":{"id":"temp_warx2xjcf9","path":"Bestiaire#/CF_SEP/Créatures du Mythe","color":"#000000"},"core":{"sourceId":"Compendium.CoC7-compendiums-fr.creatures-du-mythe.mcAn4FkJm0J1JiSa"},"CoC7":{"cocidFlag":{"id":"a.creature.larve-amorphe","lang":"fr","priority":0,"eras":{}}}},"system":{"characteristics":{"str":{"value":null,"tempValue":null,"short":"CHARAC.STR","label":"CHARAC.Strengh","formula":"(6D6+6)*5","bonusDice":0},"con":{"value":null,"tempValue":null,"short":"CHARAC.CON","label":"CHARAC.Constitution","formula":"(3D6)*5","bonusDice":0},"siz":{"value":null,"tempValue":null,"short":"CHARAC.SIZ","label":"CHARAC.Size","formula":"(6D6+12)*5","bonusDice":0},"dex":{"value":null,"tempValue":null,"short":"CHARAC.DEX","label":"CHARAC.Dexterity","formula":"(2D6+12)*5","bonusDice":0},"app":{"value":null,"tempValue":null,"short":"CHARAC.APP","label":"CHARAC.Appearance","formula":"","bonusDice":0},"int":{"value":null,"tempValue":null,"short":"CHARAC.INT","label":"CHARAC.Intelligence","formula":"(2D6+6)*5","bonusDice":0},"pow":{"value":null,"tempValue":null,"short":"CHARAC.POW","label":"CHARAC.Power","formula":"(3D6)*5","bonusDice":0},"edu":{"value":null,"tempValue":null,"short":"CHARAC.EDU","label":"CHARAC.Education","formula":"","bonusDice":0}},"attribs":{"hp":{"value":null,"max":null,"short":"HP","label":"Hit points","auto":true},"mp":{"value":null,"max":null,"short":"HP","label":"Magic points","auto":true},"lck":{"value":null,"short":"LCK","label":"Luck","max":99,"bonusDice":0},"san":{"value":null,"max":99,"short":"SAN","label":"Sanity","auto":true,"dailyLoss":0,"oneFifthSanity":" / 0","bonusDice":0},"mov":{"value":"12","max":null,"short":"MOV","label":"Movement rate","auto":false},"db":{"value":null,"short":"DB","label":"Damage bonus","auto":true},"build":{"current":null,"value":null,"short":"BLD","label":"Build","auto":true},"armor":{"value":null,"localized":false,"locations":{},"auto":false}},"conditions":{"criticalWounds":{"value":false},"unconscious":{"value":false},"dying":{"value":false},"dead":{"value":false},"prone":{"value":false},"tempoInsane":{"value":false},"indefInsane":{"value":false}},"personalDescription":{"type":"string","value":""},"description":{"keeper":""},"special":{"attribs":{"move":{"primary":{"enabled":false,"value":null,"type":null},"secondary":{"enabled":false,"value":null,"type":null}}},"sanLoss":{"checkPassed":"1","checkFailled":"1D10"},"attacksPerRound":1},"infos":{"type":""},"flags":{"locked":true,"initialized":true,"displayFormula":false},"biography":{"personalDescription":{"value":"<p><em>Lorsque les hommes de K&rsquo;n-Yan &eacute;taient descendus dans les gouffres noirs de N&rsquo;Kai, munis de grands projecteurs atomiques, ils avaient trouv&eacute; des &ecirc;tres vivants, des &ecirc;tres vivants qui glissaient le long des canaux creus&eacute;s dans la pierre et qui adoraient des images d&rsquo;onyx et de basalte repr&eacute;sentant Tsathoggua. Mais ce n&rsquo;&eacute;taient pas des crapauds comme lui. Ils &eacute;taient bien pires, c&rsquo;&eacute;tait des tas amorphes de limon noir visqueux qui pouvaient prendre temporairement une forme convenant &agrave; un but d&eacute;termin&eacute;. Les explorateurs de K&rsquo;n-Yan ne s&rsquo;attard&egrave;rent pas &agrave; faire des observations d&eacute;taill&eacute;es et ceux qui s&rsquo;&eacute;chapp&egrave;rent vivants scell&egrave;rent le passage. </em></p>\n<p>&mdash; H. P. Lovecraft et Zealia Bishop, Le Tertre</p>\n<p>&Ecirc;tres amorphes, changeant de forme instantan&eacute;ment, ils prennent aussi bien l&rsquo;apparence d&rsquo;un crapaud rudimentaire que celle d&rsquo;une chose allong&eacute;e, munie de centaines d&rsquo;embryons de pattes. Ils sont capables de suinter au travers d&rsquo;&eacute;troites fissures ou d&rsquo;agrandir leurs appendices &agrave; volont&eacute;. Ils sont fortement apparent&eacute;s &agrave; Tsathoggua, et on en rencontre souvent dans ses temples et dans des cavernes souterraines.</p>\n<p><strong>Perte de Sant&eacute; Mentale : </strong>1/1D10</p>\n<h3>Pouvoirs distinctifs</h3>\n<p><strong>Sortil&egrave;ges : </strong>Une larve amorphe a 25 % de chances de conna&icirc;tre 1 sort. Elle a alors 2 % de chances d&rsquo;en conna&icirc;tre bien plus.</p>\n<h3>Combat</h3>\n<p><strong>Attaques par round : </strong>2. Il ne peut utiliser sa morsure qu&rsquo;une fois par round.</p>\n<p><strong>Options de Combat rapproch&eacute; :</strong> Du fait de leur extr&ecirc;me fluidit&eacute; et de la rapidit&eacute; d&eacute;concertante avec laquelle elles se m&eacute;tamorphosent, les larves amorphes disposent d&rsquo;une grande vari&eacute;t&eacute; de modes d&rsquo;attaque, formant des fouets, des tentacules et d&rsquo;autres membres avec lesquels frapper et trancher leurs adversaires.</p>\n<p><strong>Morsure :</strong> La victime est instantan&eacute;ment aval&eacute;e. Le round suivant, elle encaisse 1 point de d&eacute;g&acirc;ts d&ucirc; &agrave; la constriction, puis 2 au round suivant, puis 3, 4, et ainsi de suite. Les dommages subis sont, chaque fois, sup&eacute;rieurs de 1 point &agrave; ceux du round pr&eacute;c&eacute;dent. Apr&egrave;s avoir &eacute;t&eacute; aval&eacute;e, une victime ne peut plus entreprendre aucune action, mais rien n&rsquo;emp&ecirc;che ses amis de tenter de tuer la cr&eacute;ature pour la lib&eacute;rer. Une larve continue &agrave; employer cette attaque une fois par round, tant que les TAI cumul&eacute;es des victimes ingurgit&eacute;es ne d&eacute;passent pas la sienne. Lorsqu&rsquo;elle dig&egrave;re, elle peut toujours combattre, mais il lui est alors impossible de se d&eacute;placer, &agrave; moins de r&eacute;gurgiter ce qu&rsquo;elle a aval&eacute;.</p>\n<p><strong>Saisir (man&oelig;uvre) : </strong>attrape un adversaire &agrave; l&rsquo;aide d&rsquo;un tentacule. La port&eacute;e est &eacute;gale &agrave; un cinqui&egrave;me de la TAI du monstre en m&egrave;tres.</p>\n<p><strong>Protection :</strong> invuln&eacute;rables &agrave; toutes les armes physiques, m&ecirc;me celles qui ont &eacute;t&eacute; enchant&eacute;es, car leurs blessures se referment aussi vite qu&rsquo;elles se sont ouvertes. Elles peuvent cependant &ecirc;tre affect&eacute;es par des sortil&egrave;ges, du feu, des produits chimiques, etc.</p>\n<p><strong>Note :</strong> les caract&eacute;ristiques suivantes des larves amorphes sont modifiables &agrave; la cr&eacute;ation :</p>\n<ul>\n<li>FOR : (1D6+6 x5 &agrave; 6D6+6) x5</li>\n<li>TAI : (1D6+12 x5 &agrave; 6D6+12) x5</li>\n</ul>"}}},"ownership":{"default":0,"cyefC9RG9awp41sM":3},"prototypeToken":{"flags":{},"name":"Larve amorphe","displayName":0,"width":1,"height":1,"lockRotation":false,"rotation":0,"actorLink":false,"disposition":-1,"displayBars":0,"bar1":{"attribute":"attribs.hp"},"bar2":{"attribute":"attribs.san"},"randomImg":false,"alpha":1,"light":{"alpha":1,"angle":360,"bright":0,"coloration":1,"dim":0,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"speed":5,"intensity":5,"type":null,"reverse":false},"darkness":{"min":0,"max":1},"attenuation":0.5,"color":null},"texture":{"src":"modules/coc7-module-fr-toc/portraits/creatures-formless-spawn-token.png","tint":null,"scaleX":1,"scaleY":1,"offsetX":0,"offsetY":0,"rotation":0},"sight":{"angle":360,"enabled":false,"range":0,"brightness":1,"visionMode":"basi